import java.awt.Graphics;
import java.awt.Graphics2D;
import java.awt.geom.Line2D;
import java.awt.geom.Point2D;

import javax.swing.JComponent;
import javax.swing.JFrame;


public class CheckerBoard {
	public static void main(String[] args) {
		JFrame frame = new JFrame();
		frame.setSize(600, 600);
		frame.setTitle("Hello");
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		BoardComponent comp = new BoardComponent();
		frame.add(comp);
		frame.setVisible(true);
	}
}

class BoardComponent extends JComponent {
	public void paintComponent (Graphics g) {
		Graphics2D g2 = (Graphics2D)g;
		int row = 11;
		int column = 11;
		int[][] boardData = new int[row][column];
		for (int i = 0; i < row; i++) {
			for (int j = 0; j < column; j++) {
				LetterH h = new LetterH(i, j);
				h.draw(g2);
			}
		}
		
			
	}
}

class LetterH  {
	private int xLeft;
	private int yTop;
	private int squareSize = 30;
	public LetterH(int row, int column) {
		xLeft = row*30;
		yTop = column*30;
	}
	void draw(Graphics2D g2) {
		 g2.drawRect (xLeft, yTop, squareSize, squareSize);
	}
}


